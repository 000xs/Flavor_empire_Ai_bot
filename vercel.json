{
  "crons": [
    {
      "path": "/api/scheduled-call",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/scheduled-call",
      "schedule": "0 18 * * *"
    }
  ],
  "functions": {
    "api/scheduled-call.py": {
      "runtime": "now-python@3.9"
    },
    "app.py": {
      "runtime": "now-python@3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
