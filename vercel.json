{
  "crons": [
    {
      "path": "/api/scheduled-call",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/scheduled-call",
      "schedule": "0 18 * * *"
    }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
