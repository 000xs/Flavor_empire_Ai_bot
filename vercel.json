{
  "crons": [
    {
      "path": "/api/scheduled-call",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/scheduled-call",
      "schedule": "0 18 * * *"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@4.0.0",
      "runtimeConfig": {
        "pythonVersion": "3.11"
      }
    }
  },
  "routes": [
    {
      "src": "/api/scheduled-call",
      "dest": "api/scheduled_call.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}